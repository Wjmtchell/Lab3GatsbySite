<!DOCTYPE html>
<html>
<head>
  <%- include ("../partials/header.ejs") %>
</head>

<body>

<%- include ("../partials/nav.ejs") %>

<div class="container">

<div class="users">
  <table class="users-table">
    <thead>
      <tr>
        <th> ID </th>
        <th> Username </th>
        <th> User Type </th>
      </tr>
    </thead>

    <tr>
      <% results.forEach(function(entry) { %>
        <td><%= entry.id %></td>
        <td><%= entry.username %></td>
        <td><%= entry.type %></td>
      <% }) %>
    </tr>

  </table>
</div>

</div>

</body>
</html>
